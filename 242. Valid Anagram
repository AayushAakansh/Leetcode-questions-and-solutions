// CODE

bool isAnagram(char* s, char* t) {

    
    int l = strlen(s);
    int p = strlen(t);

    if (l != p)
    {
        return false;
    }
    int count[26] = {0};

    for (int i=0; s[i] !='\0'; i++)
    {
        count[s[i] - 'a']++;
        count[t[i] - 'a']--;
    }

    for (int i=0; i<26; i++)
    {
        if (count[i] != 0)
        {
            return false;
        }
    }
    return true;
    
}
