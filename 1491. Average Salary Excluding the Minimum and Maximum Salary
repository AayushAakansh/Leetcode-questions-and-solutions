//Code

double average(int* salary, int salarySize) {

    for (int i=0; i<salarySize-1; i++)
    {
        for(int j=0; j<salarySize-i-1; j++)
        {
            if (salary[j] > salary[j+1])
            {
                int temp = salary[j];
                salary[j] = salary[j+1];
                salary[j+1] = temp;
            }
        }

    }
    int sum=0;
    for (int i=1; i<salarySize-1; i++)
    {
        sum=sum+salary[i];
    }
    double avg = (double)sum/(salarySize-2);
    return avg;

}
