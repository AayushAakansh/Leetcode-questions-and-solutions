int divide(int dividend, int divisor) {
    if (divisor == 0) {
        return INT_MAX;       }
    if (dividend == INT_MIN && divisor == -1) {
        return INT_MAX;
    }
    int sign = 1;
    if ((dividend < 0) ^ (divisor < 0)) {
        sign = -1;
    }
    long long a = llabs((long long)dividend);
    long long b = llabs((long long)divisor);
    long long count=0;
    while(a>=b)
    {
        a = a - b;
        count ++;

    }
    return sign*count; 
}
