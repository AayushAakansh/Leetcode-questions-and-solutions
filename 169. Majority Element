// Code

int majorityElement(int* nums, int numsSize) {

    int count=0;
    int result = 0;
    for (int i=0; i<numsSize; i++)
    {
        if (count==0)
        {
            result = nums[i];
        }
        if (result == nums[i])
        {
            count = count+1;
        }
        else 
        {
            count = count-1;
        }
    }

    return result;
}
