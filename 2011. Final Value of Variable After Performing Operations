// Code

int finalValueAfterOperations(char** operations, int operationsSize) {
    
    int temp = 0;
    for(int i=0; i<operationsSize; i++)
    {
        if (strcmp(operations[i],"++X") == 0 || strcmp(operations[i],"X++") == 0)
        {
            temp = temp+1;
        }
        else
        {
            temp = temp-1;
        }

    }
    return temp;
}
