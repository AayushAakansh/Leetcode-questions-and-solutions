//Code

int maximumWealth(int** accounts, int accountsSize, int* accountsColSize) {
    
    int total[100];
    for (int i=0; i< accountsSize; i++)
    {
        int sum=0;
        for (int j=0; j<*accountsColSize; j++)
        {
            sum = sum+accounts[i][j];

        }
        total[i] = sum;
    }
    int max = total[0];
    for(int i=0; i<accountsSize; i++)
    {
        if (total[i] > max)
        {
            max = total[i];
        }
    }
    return max;
}
