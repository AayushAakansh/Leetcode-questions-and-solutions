// Code

bool checkRecord(char* s) {
    
    int l = strlen(s);
    int countA = 0;
    for (int i=0; i<l; i++)
    {
        if (s[i] == 'A') {
            countA++;
            if (countA >= 2)
                return false;
    }}
    
    for (int i=0; i<l; i++)
    {
        if (s[i] == 'L' && s[i+1]=='L' && s[i+2]=='L')
        {
            return false;
            
        }
    }
    return true;
}
