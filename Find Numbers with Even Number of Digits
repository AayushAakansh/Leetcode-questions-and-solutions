// Code

int findNumbers(int* nums, int numsSize) {
    
    

    int even=0;
    for (int i = 0; i<numsSize; i++)
    {
        int temp = nums[i];
        int count=0;
        while ( temp != 0)
        {
            temp = temp/10;
            count = count+1;

        }
        if (count %2 == 0)
        {
            even = even+1;
        }

    }
    return even;
}
